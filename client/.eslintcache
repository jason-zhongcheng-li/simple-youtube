/div >\n  )\n}\n\nexport default VideoUpload;\n","/home/jasonli/workspace/github-repo/shootsta-coding-test/simple-youtube/client/src/app/video/containers/video-list.js",[],"/home/jasonli/workspace/github-repo/shootsta-coding-test/simple-youtube/client/src/app/video/components/VideoCards.js",[],"/home/jasonli/workspace/github-repo/shootsta-coding-test/simple-youtube/client/src/app/video/components/VideoCard.js",[],"/home/jasonli/workspace/github-repo/shootsta-coding-test/simple-youtube/client/src/app/video/containers/video-player.js",[],"/home/jasonli/workspace/github-repo/shootsta-coding-test/simple-youtube/client/src/app/video/components/PlayVideo.js",[],{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":21,"column":33,"nodeType":"74","messageId":"75","endLine":21,"endColumn":37},{"ruleId":"72","severity":1,"message":"76","line":21,"column":39,"nodeType":"74","messageId":"75","endLine":21,"endColumn":44},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","'error' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]